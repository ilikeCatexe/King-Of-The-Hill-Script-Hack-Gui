local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("PLANG HUB Premium Edition", "DarkTheme")
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Player event")

Plr = {}
Plr2 = {}


local drop = Section:NewDropdown("Select Player!", "Click To Select", Plr, function(t)
   Playerkill = t
end)

Section:NewButton("Kill", "", function()
local args = {
    [1] = game:GetService("Players"):WaitForChild(Playerkill).Character.Humanoid,
    [2] = math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)

Section:NewButton("Heal", "", function()
local args = {
    [1] = game:GetService("Players"):WaitForChild(Playerkill).Character.Humanoid,
    [2] = -math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)


Section:NewToggle("Auto Heal", "", function(t)
_G.Playerkill3 =  t
while _G.Playerkill3 do wait()
local args = {
    [1] = game:GetService("Players"):WaitForChild(Playerkill).Character.Humanoid,
    [2] = -math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end
end)

Section:NewToggle("Auto Kill", "", function(t)
_G.Playerkill4 =  t
while _G.Playerkill4 do wait()
    pcall(function()
local args = {
    [1] = game:GetService("Players"):WaitForChild(Playerkill).Character.Humanoid,
    [2] = math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)
end
end)

Section:NewButton("Copy Name","Refresh Dropdown", function()
    setclipboard(Playerkill)
end)

Section:NewButton("Refreshes Dropdown", "Refreshes Dropdown", function()
table.clear(Plr)
    for i,v in pairs(game.Players:GetPlayers()) do
        if v.Name ~= game.Players.LocalPlayer.Name then
            table.insert(Plr,v.Name)
            drop:Refresh(Plr)
        end
    end
end)

Section:NewLabel("King event")

Section:NewButton("set king hp = 1", "", function()
local kingxd = game:GetService("ReplicatedStorage").Values.CurrentKing.Value
local args = {
    [1] = game:GetService("Players"):WaitForChild(kingxd).Character.Humanoid,
    [2] = 7499,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)

Section:NewButton("heal king", "", function(t)

local kingxd = game:GetService("ReplicatedStorage").Values.CurrentKing.Value
local args = {
    [1] = game:GetService("Players"):WaitForChild(kingxd).Character.Humanoid,
    [2] = -math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)

Section:NewButton("kill king", "", function(t)
local kingxd = game:GetService("ReplicatedStorage").Values.CurrentKing.Value
local args = {
    [1] = game:GetService("Players"):WaitForChild(kingxd).Character.Humanoid,
    [2] = math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)


Section:NewToggle("Auto king event", "", function(t)
_G.Playerkill3 =  t
while _G.Playerkill3 do wait()
    local args = {
    [1] = "create"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("createKingEvent"):FireServer(unpack(args))
end
end)

Section:NewLabel("Misc event")

Section:NewToggle("Auto stats get", "", function(t)
_G.Playerkill4 =  t
while _G.Playerkill4 do wait()
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Stats-Get"):InvokeServer()
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Stats-Get"):InvokeServer()
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Stats-Get"):InvokeServer()
    game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Stats-Get"):InvokeServer()
end
end)


Section:NewButton("kill all whitelist", "", function(t)
for i,v in pairs(game:GetService("Players"):GetChildren()) do
if v.Name == game.Players.LocalPlayer.Name then
elseif v.Name == game.Players[Playerkill].Name then
else
local args = {
    [1] = game:GetService("Players"):WaitForChild(v.Name).Character.Humanoid,
    [2] = math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end
end
end)

Section:NewButton("kill all", "", function(t)
for i,v in pairs(game:GetService("Players"):GetChildren()) do
if v.Name == game.Players.LocalPlayer.Name then
else
local args = {
    [1] = game:GetService("Players"):WaitForChild(v.Name).Character.Humanoid,
    [2] = math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end
end
end)

Section:NewButton("heal all", "", function(t)
for i,v in pairs(game:GetService("Players"):GetChildren()) do
local args = {
    [1] = game:GetService("Players"):WaitForChild(v.Name).Character.Humanoid,
    [2] = -math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end
end)

Section:NewToggle("Auto heal all", "", function(t)
_G.ph =  t
while _G.ph do wait()
for i,v in pairs(game:GetService("Players"):GetChildren()) do
        pcall(function()
local args = {
    [1] = game:GetService("Players"):WaitForChild(v.Name).Character.Humanoid,
    [2] = -math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)
end
end
end)

Section:NewToggle("Auto heal all whitelist", "", function(t)
_G.pwh = t
while _G.wph do wait()
for i,v in pairs(game:GetService("Players"):GetChildren()) do
    if v.Name == Playerkill then
        else
        pcall(function()
local args = {
    [1] = game:GetService("Players"):WaitForChild(v.Name).Character.Humanoid,
    [2] = -math.huge,
    [3] = 99999999
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("DO_DAMAGE"):FireServer(unpack(args))
end)
end
end
end
end)

Section:NewToggle("Auto kill all", "", function(t)
_G.pk =  t
while _G.pk do wait()
